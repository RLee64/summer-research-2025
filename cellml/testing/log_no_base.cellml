<?xml version='1.0' encoding='UTF-8'?>
<model name="UnarrangedTrigEquations" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#">
    <component name="Component">
        <!-- Unknowns to solve -->
        <variable name="a" units="dimensionless"/>
        <variable initial_value="True" name="b" units="dimensionless"/>
        <variable initial_value="0" name="c" units="dimensionless"/>
        <variable name="x" units="dimensionless"/>
        <variable name="y" units="dimensionless"/>
        <!-- Initialize constants -->
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>x</ci>
                <false/>
            </apply>
            <apply>
                <eq/>
                <ci>y</ci>
                <true/>
            </apply>
            <apply>
                <eq/>
                <ci>a</ci>
                <apply>
                    <root/>
                    <degree>
                        <ci>y</ci>
                    </degree>
                    <ci>x</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>a</ci>
                <apply>
                    <or/>
                    <ci>b</ci>
                    <ci>c</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>c</ci>
                <apply>
                    <and/>
                    <ci>b</ci>
                    <ci>a</ci>
                </apply>
            </apply>
            <piecewise>
                <piece>
                <apply>
                            <times/>
                            <ci>GKr_b</ci>
                            <cn cellml:units="dimensionless">1.3</cn>
                        </apply>
                        <apply>
                            <eq/>
                            <ci>celltype</ci>
                            <cn cellml:units="dimensionless">1</cn>
                        </apply>
                    </piece>
                    <piece>
                        <apply>
                            <times/>
                            <ci>GKr_b</ci>
                            <cn cellml:units="dimensionless">0.8</cn>
                        </apply>
                        <apply>
                            <eq/>
                            <ci>celltype</ci>
                            <cn cellml:units="dimensionless">2</cn>
                        </apply>
                    </piece>
                    <otherwise>
                        <ci>GKr_b</ci>
                    </otherwise>
                </piecewise>
        </math>
        <variable initial_value="0" name="p" units="dimensionless"/>
        <variable name="t" units="second"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>p</ci>
                </apply>
                <ci>t</ci>
            </apply>
            <apply>
                <eq/>
                <ci>z3</ci>
                <ci>o</ci>
            </apply>
            

        </math>
    </component>
</model>
