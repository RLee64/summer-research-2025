<model xmlns="http://www.cellml.org/cellml/2.0#" xmlns:cellml="http://www.cellml.org/cellml/2.0#" xmlns:xlink="http://www.w3.org/1999/xlink" name="SimpleCapillary">
    <component name="Capillary">
        <!-- Variables-->
        <variable name="t" units="dimensionless"/>
        <variable name="v_y" units="dimensionless"/>
        <variable name="v_z" units="dimensionless"/>
        <variable initial_value="2.0" name="q" units="dimensionless"/>
        <variable initial_value="5.0" name="v_in" units="dimensionless"/>
        <variable initial_value="10.0" name="P_out" units="dimensionless"/>
        <variable name="P_C" units="dimensionless"/>
        <variable name="P_R_v" units="dimensionless"/>
        <variable name="P_R" units="dimensionless"/>
        <variable name="P_x" units="dimensionless"/>
        <!-- Constants-->
        <variable initial_value="10.0" name="R" units="dimensionless"/>
        <variable initial_value="10.0" name="R_v" units="dimensionless"/>
        <variable initial_value="10.0" name="C" units="dimensionless"/>
        <!-- Bond Graph Equations-->
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>v_in</ci>
                <apply>
                    <plus/>
                    <ci>v_y</ci>
                    <ci>v_z</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>P_x</ci>
                <apply>
                    <plus/>
                    <ci>P_out</ci>
                    <ci>P_R</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>P_x</ci>
                <apply>
                    <plus/>
                    <ci>P_R_v</ci>
                    <ci>P_C</ci>
                </apply>
            </apply>
            <!-- Other Constraints-->
            <apply>
                <eq/>
                <ci>P_R</ci>
                <apply>
                    <times/>
                    <ci>v_z</ci>
                    <ci>R</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>P_R_v</ci>
                <apply>
                    <times/>
                    <ci>v_y</ci>
                    <ci>R_v</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q</ci>
                </apply>
                <ci>v_y</ci>
            </apply>
            <apply>
                <eq/>
                <ci>P_C</ci>
                <apply>
                    <divide/>
                    <ci>q</ci>
                    <ci>C</ci>
                </apply>
            </apply>
        </math>
    </component>
</model>